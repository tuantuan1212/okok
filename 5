local HttpService = game:GetService("HttpService")
local serverUrl = "http://192.168.1.3:5000/data"

local function sendTest()
    local jsonData = HttpService:JSONEncode({message = "Test từ Lua"})
    local success, response = pcall(function()
        return HttpService:PostAsync(serverUrl, jsonData, Enum.HttpContentType.ApplicationJson)
    end)

    if success then
        print("[✅] Đã gửi dữ liệu thành công!")
    else
        print("[❌] Lỗi khi gửi dữ liệu:", response)
    end
end

sendTest()
